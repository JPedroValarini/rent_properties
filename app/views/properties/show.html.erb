<p style="color: green"><%= notice %></p>

<%= render @property %>

<div class="d-flex">
  <%= link_to "Edit this property", edit_property_path(@property), class: 'btn btn-warning mt-2 me-2' %>
  <%= link_to "Back to properties", home_path, class: 'btn btn-success mt-2 me-2' %>

  <%= button_to "Destroy this property", @property, method: :delete, class: 'btn btn-danger mt-2' %>
</div>
